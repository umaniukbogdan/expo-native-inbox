{
  "name": "react-native-push-notification-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "app:start": "cd apps/app && npm start",
    "app:android": "cd apps/app && npm run android",
    "app:ios": "cd apps/app && npm run ios",
    "storybook:start": "cd apps/expo-storybook && npm start",
    "storybook:android": "cd apps/expo-storybook && npm run android",
    "storybook:ios": "cd apps/expo-storybook && npm run ios",
    "storybook:generate": "cd apps/expo-storybook && npm run storybook:generate"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  }
}
